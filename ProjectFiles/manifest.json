{
  "manifest_version": 2,
  "name": "Law Abiding Citizen",
  "version": "0.1",
  "permissions": [
    "storage", "<all_urls>"
  ],
  "browser_action": {
    "default_icon": "scalesLogo.jpg",
    "default_popup": "popup.html"
    },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["jquery-3.5.1.min.js"]
    }
  ],
  "web_accessible_resources": ["laws_data_set.json", "lawsLinker.js"]
}